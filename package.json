{
  "name": "npf",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "yarn run build && node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "postinstall": "prisma generate",
    "db:push": "prisma db push --accept-data-loss",
    "db:migrate": "prisma migrate dev"
  },
  "prisma": {
    "seed": "ts-node src/seed.ts"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@fastify/cors": "^8.1.0",
    "@fastify/error": "^3.0.0",
    "@fastify/helmet": "^10.0.1",
    "@fastify/jwt": "^6.3.2",
    "@prisma/client": "^4.4.0",
    "fastify": "^4.7.0",
    "pino-pretty": "^9.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "prisma": "^4.4.0",
    "tsx": "^3.10.1",
    "typescript": "^4.8.4"
  }
}
